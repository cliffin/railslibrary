<p id="notice"><%= notice %></p>

<h1>Studentsregistereds</h1>

<table class="table table-dark table-striped <table class table table-hover">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Branch</th>
      <th>Contact</th>
      <th>Book availed</th>
      <th>Exceeded duration</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @studentsregistereds.each do |studentsregistered| %>
      <tr>
        <td><%= studentsregistered.first_name %></td>
        <td><%= studentsregistered.last_name %></td>
        <td><%= studentsregistered.branch %></td>
        <td><%= studentsregistered.contact %></td>
        <td><%= studentsregistered.book_availed %></td>
        <td><%= studentsregistered.exceeded_duration %></td>
        <td><%= link_to 'Show', studentsregistered %></td>
        <td><%= link_to 'Edit', edit_studentsregistered_path(studentsregistered) %></td>
        <td><%= link_to 'Destroy', studentsregistered, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Studentsregistered', new_studentsregistered_path, class:"btn btn-secondary" %>

